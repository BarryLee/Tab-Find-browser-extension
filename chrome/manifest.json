{
  "manifest_version": 2,

  "name": "Tab Find",
  "description": "",
  "version": "0.1",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },

  "commands": {
    "_execute_browser_action": {
      "suggested_key": {
        "default": "Ctrl+Shift+Period",
        "windows": "Ctrl+Shift+Period",
        "mac": "Command+Shift+Y",
        "chromeos": "Ctrl+Shift+U",
        "linux": "Ctrl+Shift+Period"
      }
    }
  },

  "permissions": [
    "tabs",
    "storage"
  ]
}
